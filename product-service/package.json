{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "deploy": "sls deploy",
    "invoke getProductsList": "sls invoke local --function getProductsList",
    "invoke getProductsById": "sls invoke local --function getProductsById --path test-event.json",
    "invoke createProduct": "sls invoke local --function createProduct --path test-event.json",
    "invoke catalogBatchProcess": "sls invoke local --function catalogBatchProcess --path test-event.json",
    "test": "jest"
  },
  "dependencies": {
    "aws-sdk": "^2.797.0",
    "dotenv": "^8.2.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "pg": "^8.4.2",
    "source-map-support": "^0.5.10",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "@types/serverless": "^1.72.5",
    "babel-jest": "^26.6.1",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.6.3",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
